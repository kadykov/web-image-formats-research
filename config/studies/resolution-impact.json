{
  "$schema": "../study.schema.json",
  "id": "resolution-impact",
  "name": "Resolution Impact Study",
  "description": "Study how input resolution affects compression efficiency across different image formats (JPEG, WebP, AVIF, JPEG XL). Tests the same images at multiple downscaled resolutions to understand resolution-dependent performance characteristics.",
  "dataset": {
    "id": "div2k-valid",
    "max_images": 10
  },
  "encoders": [
    {
      "format": "jpeg",
      "quality": 85,
      "resolution": [2048, 1280, 720, 480, 320, 240, 200, 160, 120]
    },
    {
      "format": "webp",
      "quality": 87,
      "method": 6,
      "resolution": [2048, 1280, 720, 480, 320, 240, 200, 160, 120]
    },
    {
      "format": "avif",
      "quality": 65,
      "speed": 3,
      "resolution": [2048, 1280, 720, 480, 320, 240, 200, 160, 120]
    },
    {
      "format": "jxl",
      "quality": 85,
      "effort": 9,
      "resolution": [2048, 1280, 720, 480, 320, 240, 200, 160, 120]
    }
  ]
}
